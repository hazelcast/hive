{
  "name": "@hazelcast/ui",
  "version": "1.3.2",
  "description": "Hazelcast design system components",
  "author": "",
  "homepage": "https://github.com/hazelcast/frontend-shared#readme",
  "license": "Apache-2.0",
  "main": "lib/index.js",
  "module": "lib/index.js",
  "sideEffects": false,
  "directories": {
    "lib": "lib",
    "test": "__tests__"
  },
  "files": [
    "lib",
    "styles"
  ],
  "publishConfig": {
    "access": "public"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/hazelcast/frontend-shared.git"
  },
  "scripts": {
    "compile": "shx rm -f *.tsbuildinfo && shx rm -rf lib && shx mkdir lib && shx cp -r src/* lib && shx rm -rf lib/*.{tsx,ts} && shx rm -rf lib/**/*.{tsx,ts} && tsc -p tsconfig.build.json",
    "test": "jest --maxWorkers=50%",
    "test:watch": "jest --watch",
    "start": "pnpm run storybook",
    "storybook": "storybook dev -p 6006",
    "build-storybook": "storybook build",
    "publish-storybook": "chromatic --storybook-build-dir storybook-static --project-token $CHROMATIC_PROJECT_TOKEN --exit-zero-on-changes",
    "chromatic": "chromatic --project-token=CHROMATIC_PROJECT_TOKEN",
    "test:visual": "loki --verboseRenderer --requireReference --chromeTolerance 3 --reactUri file:./storybook-static --chromeDockerImage 'yukinying/chrome-headless-browser-stable:110.0.5481.77' --chromeFlags='--headless --disable-gpu --hide-scrollbars --no-sandbox --allow-file-access-from-files'",
    "test:visual:approve": "loki approve",
    "generate-screenshots": "pnpm run build-storybook && loki update --reactUri file:./storybook-static --chromeDockerImage 'yukinying/chrome-headless-browser-stable:110.0.5481.77' --chromeFlags='--headless --disable-gpu --hide-scrollbars --no-sandbox --allow-file-access-from-files'",
    "lint": "pnpm run lint:ts && pnpm run lint:css",
    "lint:fix": "pnpm run lint:ts:fix && pnpm run lint:css:fix",
    "lint:ts": "eslint .",
    "lint:ts:fix": "pnpm run lint:ts --fix",
    "lint:css": "postcss --config ./.postcss-lint --replace '**/*.{css,scss}'",
    "lint:css:fix": "stylelint --fix '**/*.{css,scss}'"
  },
  "bugs": {
    "url": "https://github.com/hazelcast/frontend-shared/issues"
  },
  "dependencies": {
    "@hazelcast/helpers": "^1.3.2",
    "@hazelcast/services": "^1.3.2",
    "@headlessui/react": "^1.7.19",
    "@icons-pack/react-simple-icons": "^12.0.0",
    "@popperjs/core": "^2.11.8",
    "@tanstack/react-table": "^8.21.2",
    "@types/react-modal": "3.16.3",
    "classnames": "^2.5.1",
    "date-fns": "^2.30.0",
    "dequal": "^2.0.3",
    "formik": "^2.4.6",
    "nuka-carousel": "^5.6.0",
    "rc-tooltip": "^5.3.1",
    "react-datepicker": "^4.25.0",
    "react-feather": "^2.0.10",
    "react-hot-keys": "^2.7.3",
    "react-merge-refs": "^1.1.0",
    "react-modal": "^3.16.3",
    "react-popper": "^2.3.0",
    "react-select": "^5.10.0",
    "react-simple-timefield": "^3.3.1",
    "react-uid": "^2.4.0",
    "react-use": "^17.6.0",
    "use-deep-compare": "^1.3.0",
    "use-persisted-state": "^0.3.3"
  },
  "devDependencies": {
    "@chromatic-com/storybook": "^3.2.5",
    "@hazelcast/test-helpers": "^1.3.2",
    "@storybook/addon-designs": "^8.2.0",
    "@storybook/addon-essentials": "^8.6.3",
    "@storybook/addon-interactions": "^8.6.3",
    "@storybook/addon-onboarding": "^8.6.3",
    "@storybook/addon-webpack5-compiler-swc": "^2.1.0",
    "@storybook/blocks": "^8.6.3",
    "@storybook/manager-api": "^8.6.3",
    "@storybook/react": "^8.6.3",
    "@storybook/react-webpack5": "^8.6.3",
    "@storybook/test": "^8.6.3",
    "@storybook/theming": "8.5.8",
    "@testing-library/dom": "^10.4.0",
    "@testing-library/jest-dom": "^6.6.3",
    "@testing-library/react": "^16.2.0",
    "@testing-library/user-event": "^14.6.1",
    "@types/classnames": "^2.3.4",
    "@types/jest-axe": "^3.5.9",
    "@types/node": "^18.15.3",
    "@types/react": "^18.3.18",
    "@types/react-datepicker": "^4.19.6",
    "@types/react-dom": "^18.3.5",
    "@types/use-persisted-state": "^0.3.5",
    "babel-loader": "^9.2.1",
    "doiuse": "^6.0.5",
    "jest-axe": "^9.0.0",
    "loki": "^0.35.1",
    "postcss": "^8.4.47",
    "postcss-cli": "^11.0.0",
    "postcss-reporter": "^7.1.0",
    "postcss-scss": "^4.0.9",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "storybook": "^8.6.3",
    "stylelint": "^16.9.0",
    "stylelint-config-standard": "^36.0.1",
    "stylelint-config-standard-scss": "^13.1.0",
    "stylelint-prettier": "^5.0.2",
    "stylelint-scss": "^6.7.0",
    "yup": "^0.32.11"
  },
  "peerDependencies": {
    "@types/lodash": "^4.17.13",
    "@types/react": "^18",
    "@types/react-dom": "^18",
    "lodash": "^4.17.21",
    "react": "^18",
    "react-dom": "^18"
  }
}
